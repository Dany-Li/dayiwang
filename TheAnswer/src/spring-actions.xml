<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">
	
	
	
	<bean id="manageAction"  class="action.ManageAction">

	<property name="manageDao"><ref bean="manageDao"/> </property>

	<property name="userDao"><ref bean="userDao"/> </property>
	<property name="gonggaoDao"><ref bean="gonggaoDao"/> </property>
	<property name="luntanDao"><ref bean="luntanDao"/> </property>
	<property name="taolunDao"><ref bean="taolunDao"/> </property>

	<property name="fenleiDao"><ref bean="fenleiDao"/> </property>
	
	
	</bean>
	
	
	<bean id="indexAction"  class="action.IndexAction">
	
	<property name="userDao"><ref bean="userDao"/> </property>
	
     <property name="gonggaoDao"><ref bean="gonggaoDao"/> </property>
    <property name="luntanDao"><ref bean="luntanDao"/> </property>
	<property name="taolunDao"><ref bean="taolunDao"/> </property>
	
	<property name="fenleiDao"><ref bean="fenleiDao"/> </property>
	</bean>
	
	
	
	
</beans>
